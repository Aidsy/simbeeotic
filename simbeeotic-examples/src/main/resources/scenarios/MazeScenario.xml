<?xml version="1.0"?>

<scenario xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://harvard/robobees/simbeeotic/configuration/scenario"
          xsi:schemaLocation="http://harvard/robobees/simbeeotic/configuration/scenario http://some/path/to/schema.xsd">

	<master-seed>
		<constant value="111982" />
	</master-seed>

	<simulation>
		<end-time>150000</end-time>
	</simulation>

	<models>
		<model>
			<java-class>harvard.robobees.simbeeotic.example.MazeBee</java-class>
			<start-position x="0.5" y="0.5" z="0.5"></start-position>
		</model>
	</models>

	<components>
		<variation>
			<java-class>harvard.robobees.simbeeotic.component.VisComponent3D</java-class>
		</variation>
	</components>

</scenario>